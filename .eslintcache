/src/components/NewsCard/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/pages/Home/index.js",["166"],"/Users/Tyler/Development/code/react/guardian-test/src/components/Footer/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/pages/styles.js",[],["167","168"],"/Users/Tyler/Development/code/react/guardian-test/src/store/store.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/store/actions/news.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/pages/Article/index.js",["169","170"],"import React, { useState, useEffect } from 'react';\nimport { connect } from 'react-redux';\nimport dayjs from 'dayjs';\nimport DOMPurify from 'dompurify';\nimport { v4 as uuidv4 } from 'uuid';\n\nimport { addBookmark, removeBookmark } from '../../store/actions/bookmark';\nimport { fetchArticle } from '../../store/actions/news';\nimport BookmarkButton from '../../components/BookmarkButton';\nimport Layout from '../../components/Layout';\nimport placeholder from '../../assets/images/placeholder.svg';\n\nimport {\n  ArticleContainer,\n  Caption,\n  Container,\n  Date,\n  Divider,\n  Headline,\n  Image,\n  ImageContainer,\n  PageHeader,\n  Title,\n} from './styles';\n\nfunction Article({\n  addBookmark,\n  article,\n  bodyHtml,\n  bookmarks,\n  date,\n  fetchArticle,\n  headline,\n  imageAlt,\n  imageCaption,\n  imageSrc,\n  location,\n  removeBookmark,\n  title,\n}) {\n  const id = location.pathname;\n\n  const [bookmarked, setBookmarked] = useState(null);\n\n  useEffect(() => {\n    fetchArticle(id);\n  }, []);\n\n  useEffect(() => {\n    \n    console.log('bookmarks ', bookmarks);\n\n    if (bookmarks) {\n      let hasArticle = bookmarks.some(\n        (articleObj) => articleObj.article.id === article.id\n      );\n      console.log('article ', article);\n      console.log('hasArticle ', hasArticle);\n      setBookmarked(hasArticle);\n    }\n  }, [bookmarks]);\n\n  const sanitizeBodyHtml = DOMPurify.sanitize(bodyHtml, {\n    ALLOWED_TAGS: ['h1', 'p', 'span'],\n    ALLOWED_ATTR: ['style'],\n  });\n\n  const sanitizeHeadlineHtml = DOMPurify.sanitize(headline, {\n    ALLOWED_TAGS: ['h1', 'p', 'span'],\n    ALLOWED_ATTR: ['style'],\n  });\n\n  const handleBookmark = (article) => {\n    const payload = { article: article, bookmarked: true, id: uuidv4() };\n    addBookmark(payload);\n\n    // if (bookmarked) {\n    //   removeBookmark(article);\n    // } else {\n    //   const payload = { article: article, bookmarked: true, id: uuidv4() };\n    //   addBookmark(payload);\n    // }\n  };\n\n  return (\n    <Layout>\n      <Container>\n        <PageHeader>\n          <BookmarkButton\n            text={bookmarked ? 'Remove from bookmark' : 'Add to bookmark'}\n            handleBookmark={() => handleBookmark(article)}\n          />\n          <Date>{dayjs(date).format('ddd D MMM YYYY hh:mm')}</Date>\n          <Title>{title}</Title>\n          <Headline>\n            <div dangerouslySetInnerHTML={{ __html: sanitizeHeadlineHtml }} />\n          </Headline>\n          <Divider />\n        </PageHeader>\n        <ArticleContainer>\n          <div dangerouslySetInnerHTML={{ __html: sanitizeBodyHtml }} />\n        </ArticleContainer>\n        <ImageContainer>\n          {imageSrc ? (\n            <Image src={imageSrc} alt={imageAlt} />\n          ) : (\n            <img\n              src={placeholder}\n              alt=\"The Peaks placeholder\"\n              width=\"100%\"\n              height=\"auto\"\n            />\n          )}\n          <Caption>{imageCaption}</Caption>\n        </ImageContainer>\n      </Container>\n    </Layout>\n  );\n}\n\nconst mapStateToProps = (state) => {\n  const { article } = state.articleReducer;\n\n  if (article) {\n    const {\n      headline,\n      body,\n      thumbnail,\n      trailText,\n      firstPublicationDate,\n    } = article.fields;\n\n    const { alt, caption } = article.blocks.main.elements[0].imageTypeData;\n\n    return {\n      bookmarks: state.bookmarkReducer.bookmarks,\n      article: article ? article : '',\n      title: headline ? headline : '',\n      bodyHtml: body ? body : '',\n      imageSrc: thumbnail ? thumbnail : '',\n      headline: trailText ? trailText : '',\n      date: firstPublicationDate ? firstPublicationDate : '',\n      imageAlt: alt ? alt : '',\n      imageCaption: caption ? caption : '',\n    };\n  }\n};\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    fetchArticle: (section, articleId) =>\n      dispatch(fetchArticle(section, articleId)),\n    addBookmark: (article) => dispatch(addBookmark(article)),\n    removeBookmark: (article) => dispatch(removeBookmark(article)),\n  };\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Article);\n","/Users/Tyler/Development/code/react/guardian-test/src/components/Search/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/pages/Article/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/pages/Category/index.js",["171"],"/Users/Tyler/Development/code/react/guardian-test/src/components/BookmarkButton/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/BookmarkButton/styles.js",[],["172","173"],"/Users/Tyler/Development/code/react/guardian-test/src/components/TopStories/index.js",["174"],"/Users/Tyler/Development/code/react/guardian-test/src/components/TopStories/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/pages/Home/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/pages/SearchResults/index.js",["175"],"/Users/Tyler/Development/code/react/guardian-test/src/pages/SavedArticles/index.js",["176","177"],"/Users/Tyler/Development/code/react/guardian-test/src/components/PageHeader/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/PageHeader/styles.js",[],["178","179"],"/Users/Tyler/Development/code/react/guardian-test/src/store/reducers/rootReducer.js",[],"/Users/Tyler/Development/code/react/guar