ue} />\n          <NewsSection>{newsCards}</NewsSection>\n        </Container>\n      </motion.div>\n    </Layout>\n  );\n}\n\nconst mapStateToProps = (state) => {\n  const { bookmarks } = state.bookmarkReducer;\n\n  if (bookmarks) {\n    return {\n      bookmarks,\n    };\n  }\n};\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    getBookmarks: () => dispatch(getBookmarks()),\n    removeBookmark: (articleId) => dispatch(removeBookmark(articleId)),\n  };\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(SavedArticles);\n","/Users/Tyler/Development/code/react/guardian-test/src/pages/styles.js",[],{"ruleId":"108","severity":1,"message":"109","line":21,"column":6,"nodeType":"110","endLine":21,"endColumn":8,"suggestions":"111"},{"ruleId":"108","severity":1,"message":"112","line":15,"column":6,"nodeType":"110","endLine":15,"endColumn":8,"suggestions":"113"},{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","replacedBy":"117"},{"ruleId":"108","severity":1,"message":"109","line":23,"column":6,"nodeType":"110","endLine":23,"endColumn":17,"suggestions":"118"},{"ruleId":"108","severity":1,"message":"119","line":46,"column":6,"nodeType":"110","endLine":46,"endColumn":8,"suggestions":"120"},{"ruleId":"108","severity":1,"message":"121","line":17,"column":6,"nodeType":"110","endLine":17,"endColumn":8,"suggestions":"122"},{"ruleId":"123","severity":1,"message":"124","line":20,"column":32,"nodeType":"125","messageId":"126","endLine":20,"endColumn":34},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'fetchArticles'. Either include it or remove the dependency array. If 'fetchArticles' changes too often, find the parent component that defines it and wrap that definition in useCallback.","ArrayExpression",["127"],"React Hook useEffect has missing dependencies: 'fetchTopStories' and 'section'. Either include them or remove the dependency array. If 'fetchTopStories' changes too often, find the parent component that defines it a