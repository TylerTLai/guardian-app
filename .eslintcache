nimport NewsSection from \"../../components/NewsSection\";\nimport TopStories from \"../../components/TopStories\";\n\nimport { ArticleLink, Container, Heading, SpinnerContainer } from \"../styles\";\nimport theme from \"../../styles/theme\";\nimport { routeTransitionVariants } from \"../../animations/routeTransitions\";\n\nconst { colors } = theme;\n\nfunction Home({ articles, fetchArticles, loading }) {\n  useEffect(() => {\n    const section = \"sport\";\n    fetchArticles(section);\n  }, []);\n\n  const newsCards = articles.slice(0, 3).map((article) => {\n    const imageUrl = article.fields.thumbnail;\n    return (\n      <ArticleLink to={\"/\" + article.id} key={article.id}>\n        <NewsCard title={article.webTitle} imageSrc={imageUrl} />\n      </ArticleLink>\n    );\n  });\n\n  return (\n    <Layout>\n      <Container>\n        {loading ? (\n          <SpinnerContainer>\n            <FadingCircle size={100} color={colors.gray} />\n          </SpinnerContainer>\n        ) : (\n          <motion.div\n            animate=\"in\"\n            exit=\"out\"\n            initial=\"out\"\n            variants={routeTransitionVariants}\n          >\n            <TopStories section=\"world\" />\n            <Heading>Sports</Heading>\n            <NewsSection>{newsCards}</NewsSection>\n          </motion.div>\n        )}\n      </Container>\n    </Layout>\n  );\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    articles: state.articleReducer.articles,\n    loading: state.articleReducer.loading,\n  };\n};\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    fetchArticles: (section) => dispatch(fetchArticles(section)),\n  };\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Home);\n",["145","146"],"/Users/Tyler/Development/code/react/guardian-test/src/components/TopStories/makeNewsArticles.js",[],["147","148"],"/Users/Tyler/Development/code/react/guardian-test/src/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/App/index.js",[],["149","150"],"/Users/Tyler/Development/code/react/guardian-test/src/styles/globalStyles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/Search/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/TopStories/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/TopStories/index.js",["151"],"/Users/Tyler/Development/code/react/guardian-test/src/pages/SearchResults/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/Navbar/index.js",[],["152","153"],"/Users/Tyler/Development/code/react/guardian-test/src/pages/Category/index.js",["154"],"/Users/Tyler/Development/code/react/guardian-test/src/styles/theme.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/PageHeader/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/NewsSection/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/Header/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/store/reducers/rootReducer.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/BookmarkButton/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/NewsCard/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/Search/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/pages/Article/index.js",["155"],"import React, { useEffect } from \"react\";\nimport { connect } from \"react-redux\";\nimport dayjs from \"dayjs\";\nimport DOMPurify from \"dompurify\";\nimport { motion } from \"framer-motion\";\n\nimport { addBookmark, removeBookmark } from \"../../store/actions/bookmark\";\nimport BookmarkButton from \"../../components/BookmarkButton\";\nimport { fetchArticle } from \"../../store/actions/articles\";\nimport Layout from \"../../components/Layout\";\nimport placeholder from \"../../assets/images/placeholder.svg\";\nimport { routeTransitionVariants } from \"../../animations/routeTransitions\";\n\nimport {\n  ArticleContainer,\n  Caption,\n  Container,\n  Date,\n  Divider,\n  Headline,\n  Image,\n  ImageContainer,\n  PageHeader,\n  Title,\n} from \"./styles\";\n\nfunction Article({\n  addBookmark,\n  article,\n  bodyHtml,\n  bookmarks,\n  date,\n  fetchArticle,\n  headline,\n  imageAlt,\n  imageCaption,\n  imageSrc,\n  location,\n  removeBookmark,\n  title,\n}) {\n  const id = location.pathname;\n\n  useEffect(() => {\n    fetchArticle(id);\n  }, []);\n\n  const sanitizeBodyHtml = DOMPurify.sanitize(bodyHtml, {\n    ALLOWED_TAGS: [\"h1\", \"p\", \"span\"],\n    ALLOWED_ATTR: [\"style\"],\n  });\n\n  const sanitizeHeadlineHtml = DOMPurify.sanitize(headline, {\n    ALLOWED_TAGS: [\"h1\", \"p\", \"span\"],\n    ALLOWED_ATTR: [\"style\"],\n  });\n\n  const handleBookmark = (article) => {\n    if (bookmarked) {\n      removeBookmark(article.id);\n    } else {\n      addBookmark(article);\n    }\n  };\n\n  const bookmarked = bookmarks\n    ? bookmarks.some((bookmark) => bookmark.id === article.id)\n    : false;\n\n  return (\n    <Layout>\n      <motion.div\n        animate=\"in\"\n        exit=\"out\"\n        initial=\"out\"\n        variants={routeTransitionVariants}\n      >\n        <Container>\n          <PageHeader>\n            <BookmarkButton\n              text={bookmarked ? \"Remove from bookmark\" : \"Add to bookmark\"}\n              handleBookmark={() => handleBookmark(article, bookmarked)}\n            />\n            <Date>{dayjs(date).format(\"ddd D MMM YYYY hh:mm\")}</Date>\n            <Title>{title}</Title>\n            <Headline>\n              <div dangerouslySetInnerHTML={{ __html: sanitizeHeadlineHtml }} />\n            </Headline>\n            <Divider />\n          </PageHeader>\n          <ArticleContainer>\n            <div dangerouslySetInnerHTML={{ __html: sanitizeBodyHtml }} />\n          </ArticleContainer>\n          <ImageContainer>\n            {imageSrc ? (\n              <Image src={imageSrc} alt={imageAlt} />\n            ) : (\n              <Image\n                src={placeholder}\n                alt=\"The Peaks placeholder\"\n                width=\"100%\"\n                height=\"auto\"\n              />\n            )}\n            <Caption>{imageCaption}</Caption>\n          </ImageContainer>\n        </Container>\n      </motion.div>\n    </Layout>\n  );\n}\n\nconst mapStateToProps = (state) => {\n  const { article } = state.articleReducer;\n\n  if (article) {\n    const {\n      headline,\n      body,\n      thumbnail,\n      trailText,\n      firstPublicationDate,\n    } = article.fields;\n\n    const { alt, caption } = article.blocks.main\n      ? article.blocks.main.elements[0].imageTypeData\n      : \"\";\n\n    return {\n      article: article ? article : \"\",\n      bodyHtml: body ? body : \"\",\n      bookmarks: state.bookmarkReducer.bookmarks,\n      date: firstPublicationDate ? firstPublicationDate : \"\",\n      headline: trailText ? trailText : \"\",\n      imageAlt: alt ? alt : \"\",\n      imageCaption: caption ? caption : \"\",\n      imageSrc: thumbnail ? thumbnail : \"\",\n      title: headline ? headline : \"\",\n    };\n  }\n};\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    fetchArticle: (section, articleId) =>\n      dispatch(fetchArticle(section, articleId)),\n    addBookmark: (article) => dispatch(addBookmark(article)),\n    removeBookmark: (article) => dispatch(removeBookmark(article)),\n  };\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Article);\n","/Users/Tyler/Development/code/react/guardian-test/src/components/Layout/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/animations/routeTransitions.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/pages/SavedArticles/index.js",["156","157"],"/Users/Tyler/Development/code/react/guardian-test/src/pages/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/componen