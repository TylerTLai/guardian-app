geHeader\";\nimport { fetchTopStories } from \"../../store/actions/articles\";\n\nimport { routeTransitionVariants } from \"../../animations/routeTransitions\";\nimport { Container } from \"./styles\";\n\nfunction TopStories({ fetchTopStories, section, sortValue, topStories }) {\n  useEffect(() => {\n    fetchTopStories(section, sortValue);\n  }, [sortValue]);\n\n  const selectedStories = makeNewsArticles(topStories, 8);\n\n  return (\n    <Container>\n      <PageHeader\n        bookmarkText=\"VIEW BOOKMARK\"\n        filter={true}\n        title=\"Top stories\"\n      />\n\n      <motion.div\n        animate=\"display\"\n        initial=\"initial\"\n        variants={routeTransitionVariants}\n      >\n        {selectedStories}\n      </motion.div>\n    </Container>\n  );\n}\n\nconst mapStateToProps = (state) => {\n  console.log(\"top stories msp \", state.articleReducer.sortValue);\n  return {\n    topStories: state.articleReducer.topStories,\n    sortValue: state.articleReducer.sortValue,\n  };\n};\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    fetchTopStories: (section) => dispatch(fetchTopStories(section)),\n  };\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(TopStories);\n","/Users/Tyler/Development/code/react/guardian-test/src/pages/SearchResults/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/Navbar/index.js",[],["139","140"],"/Users/Tyler/Development/code/react/guardian-test/src/pages/Category/index.js",["141"],"/Users/Tyler/Development/code/react/guardian-test/src/styles/theme.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/PageHeader/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/NewsSection/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/Header/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/store/reducers/rootReducer.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/BookmarkButton/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/NewsCard/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/Search/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/pages/Article/index.js",["142"],"import React, { useEffect } from \"react\";\nimport { connect } from \"react-redux\";\nimport dayjs from \"dayjs\";\nimport DOMPurify from \"dompurify\";\nimport { motion } from \"framer-motion\";\n\nimport { addBookmark, removeBookmark } from \"../../store/actions/bookmark\";\nimport BookmarkButton from \"../../components/BookmarkButton\";\nimport { fetchArticle } from \"../../store/actions/articles\";\nimport Layout from \"../../components/Layout\";\nimport placeholder from \"../../assets/images/placeholder.svg\";\nimport { routeTransitionVariants } from \"../../animations/routeTransitions\";\n\nimport {\n  ArticleContainer,\n  Caption,\n  Container,\n  Date,\n  Divider,\n  Headline,\n  Image,\n  ImageContainer,\n  PageHeader,\n  Title,\n} from \"./styles\";\n\nfunction Article({\n  addBookmark,\n  article,\n  bodyHtml,\n  bookmarks,\n  date,\n  fetchArticle,\n  headline,\n  imageAlt,\n  imageCaption,\n  imageSrc,\n  location,\n  removeBookmark,\n  title,\n}) {\n  const id = location.pathname;\n\n  useEffect(() => {\n    fetchArticle(id);\n  }, []);\n\n  const sanitizeBodyHtml = DOMPurify.sanitize(bodyHtml, {\n    ALLOWED_TAGS: [\"h1\", \"p\", \"span\"],\n    ALLOWED_ATTR: [\"style\"],\n  });\n\n  const sanitizeHeadlineHtml = DOMPurify.sanitize(headline, {\n    ALLOWED_TAGS: [\"h1\", \"p\", \"span\"],\n    ALLOWED_ATTR: [\"style\"],\n  });\n\n  const handleBookmark = (article) => {\n    if (bookmarked) {\n      removeBookmark(article.id);\n    } else {\n      addBookmark(article);\n    }\n  };\n\n  const bookmarked = bookmarks\n    ? bookmarks.some((bookmark) => bookmark.id === article.id)\n    : false;\n\n  return (\n    <Layout>\n      <motion.div\n        animate=\"in\"\n        exit=\"out\"\n        initial=\"out\"\n        variants={routeTransitionVariants}\n      >\n        <Container>\n          <PageHeader>\n            <BookmarkButton\n              text={bookmarked ? \"Remove from bookmark\" : \"Add to bookmark\"}\n              handleBookmark={() => handleBookmark(article, bookmarked)}\n            />\n            <Date>{dayjs(date).format(\"ddd D MMM YYYY hh:mm\")}</Date>\n            <Title>{title}</Title>\n            <Headline>\n              <div dangerouslySetInnerHTML={{ __html: sanitizeHeadlineHtml }} />\n            </Headline>\n            <Divider />\n          </PageHeader>\n          <ArticleContainer>\n            <div dangerouslySetInnerHTML={{ __html: sanitizeBodyHtml }} />\n          </ArticleContainer>\n          <ImageContainer>\n            {imageSrc ? (\n              <Image src={imageSrc} alt={imageAlt} />\n            ) : (\n              <Image\n                src={placeholder}\n                alt=\"The Peaks placeholder\"\n                width=\"100%\"\n                height=\"auto\"\n              />\n            )}\n            <Caption>{imageCaption}</Caption>\n          </ImageContainer>\n        </Container>\n      </motion.div>\n    </Layout>\n  );\n}\n\nconst mapStateToProps = (state) => {\n  const { article } = state.articleReducer;\n\n  if (article) {\n    const {\n      headline,\n      body,\n      thumbnail,\n      trailText,\n      firstPublicationDate,\n    } = article.fields;\n\n    const { alt, caption } = article.blocks.main\n      ? article.blocks.main.elements[0].imageTypeData\n      : \"\";\n\n    return {\n      article: article ? article : \"\",\n      bodyHtml: body ? body : \"\",\n      bookmarks: state.bookmarkReducer.bookmarks,\n      date: firstPublicationDate ? firstPublicationDate : \"\",\n      headline: trailText ? trailText : \"\",\n      imageAlt: alt ? alt : \"\",\n      imageCaption: caption ? caption : \"\",\n      imageSrc: thumbnail ? thumbnail : \"\",\n      title: headline ? headline : \"\",\n    };\n  }\n};\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    fetchArticle: (section, articleId) =>\n      dispatch(fetchArticle(section, articleId)),\n    addBookmark: (article) => dispatch(addBookmark(article)),\n    removeBookmark: (article) => dispatch(removeBookmark(article)),\n  };\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Article);\n","/Users/Tyler/Development/code/react/guardian-test/src/components/Layout/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/animations/routeTransitions.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/pages/SavedArticles/index.js",["143","144"],"/Users/Tyler/Development/code/react/guardian-test/src/pages/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/BookmarkButton/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/Footer/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/NewsCard/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/PageHeader/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/pages/Article/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/store/reducers/articleReducer.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/store/reducers/bookmarkReducer.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/store/reducers/searchReducer.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/store/actions/articles.js",[],{"ruleId":"145","severity":1,"message":"146","line":22,"column":6,"nodeType":"147","endLine":22,"endColumn":8,"suggestions":"148"},{"ruleId":"145","severity":1,"message":"149","line":15,"column":6,"nodeType":"147","endLine":15,"endColumn":17,"suggestions":"150"},{"ruleId":"151","replacedBy":"152"},{"ruleId":"153","replacedBy":"154"},{"ruleId":"145","severity":1,"message":"146","line":23,"column":6,"nodeType":"147