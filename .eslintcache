0","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/Tyler/Development/code/react/guardian-test/src/pages/Home/index.js",["151"],"import React, { useEffect } from \"react\";\nimport { connect } from \"react-redux\";\nimport { FadingCircle } from \"better-react-spinkit\";\nimport { motion } from \"framer-motion\";\n\nimport { fetchArticles } from \"../../store/actions/articles\";\nimport Layout from \"../../components/Layout\";\nimport NewsCard from \"../../components/NewsCard\";\nimport NewsSection from \"../../components/NewsSection\";\nimport TopStories from \"../../components/TopStories\";\n\nimport { ArticleLink, Container, Heading, SpinnerContainer } from \"../styles\";\nimport theme from \"../../styles/theme\";\nimport { routeTransitionVariants } from \"../../animations/routeTransitions\";\n\nconst { colors } = theme;\n\nfunction Home({ articles, fetchArticles, loading }) {\n  useEffect(() => {\n    const section = \"sport\";\n    fetchArticles(section);\n  }, []);\n\n  const newsCards = articles.slice(0, 3).map((article) => {\n    const imageUrl = article.fields.thumbnail;\n    return (\n      <ArticleLink to={\"/\" + article.id} key={article.id}>\n        <NewsCard title={article.webTitle} imageSrc={imageUrl} />\n      </ArticleLink>\n    );\n  });\n\n  return (\n    <Layout>\n      <Container>\n        {loading ? (\n          <SpinnerContainer>\n            <FadingCircle size={100} color={colors.gray} />\n          </SpinnerContainer>\n        ) : (\n          <motion.div\n            animate=\"in\"\n            exit=\"out\"\n            initial=\"out\"\n            variants={routeTransitionVariants}\n          >\n            <TopStories section=\"world\" />\n            <Heading>Sports</Heading>\n            <NewsSection>{newsCards}</NewsSection>\n          </motion.div>\n        )}\n      </Container>\n    </Layout>\n  );\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    articles: state.articleReducer.articles,\n    loading: state.articleReducer.loading,\n  };\n};\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    fetchArticles: (section) => dispatch(fetchArticles(section)),\n  };\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Home);\n",["152","153"],"/Users/Tyler/Development/code/react/guardian-test/src/components/TopStories/makeNewsArticles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/App/index.js",[],["154","155"],"/Users/Tyler/Development/code/react/guardian-test/src/styles/globalStyles.js",[],["156","157"],"/Users/Tyler/Development/code/react/guardian-test/src/components/Search/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/TopStories/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/TopStories/index.js",["158"],"/Users/Tyler/Development/code/react/guardian-test/src/pages/SearchResults/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/Navbar/index.js",[],["159","160"],"/Users/Tyler/Development/code/react/guardian-test/src/pages/Category/index.js",["161"],"/Users/Tyler/Development/code/react/guardian-test/src/styles/theme.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/PageHeader/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/NewsSection/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/Header/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/store/reducers/rootReducer.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/BookmarkButton/index.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/NewsCard/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/components/Search/styles.js",[],"/Users/Tyler/Development/code/react/guardian-test/src/pages/Article/index.js",["162"],"/Users/Tyler/Develop